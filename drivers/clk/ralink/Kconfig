# MediaTek Mt7621 Clock Driver
#
menu "Clock driver for Mediatek mt7621 SoC"
	depends on SOC_MT7621 || COMPILE_TEST

config CLK_MT7621
	bool "Clock driver for MediaTek MT7621"
	depends on SOC_MT7621 || COMPILE_TEST
	default SOC_MT7621
	select MFD_SYSCON
	help
	  This driver supports MediaTek MT7621 basic clocks.
endmenu
